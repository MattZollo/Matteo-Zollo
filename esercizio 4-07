from ast import AnnAssign
from pickle import FALSE


class primaclasse(): 

  Anni = input("Quanti anni hai?")

  if Anni != FALSE :
    
   print(Anni)

  else:

   print("errore")

  
  Lista = [Anni]

  Anni1 = input("Quanti anni hai?")
  Anni2 = input("Quanti anni hai?")
  Anni3 = input("Quanti anni hai?")
  Anni4 = input("Quanti anni hai?")
  Lista.append (Anni1)
  Lista.append (Anni2)
  Lista.append (Anni3)
  Lista.append (Anni4)

  if Anni1 != FALSE and Anni2 != FALSE and Anni3 != FALSE and Anni4 != FALSE :

   print("Errore")

  else:
   print(Lista) 
